                        <!-- Alert Messages -->
                        <% if (error) { %>
                            <div class="mb-6 p-4 bg-red-50 border border-red-200 rounded-xl animate-bounce-in">
                                <div class="flex">
                                    <div class="flex-shrink-0">
                                        <svg class="h-5 w-5 text-red-400" viewBox="0 0 20 20" fill="currentColor">
                                            <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM8.707 7.293a1 1 0 00-1.414 1.414L8.586 10l-1.293 1.293a1 1 0 101.414 1.414L10 11.414l1.293 1.293a1 1 0 001.414-1.414L11.414 10l1.293-1.293a1 1 0 00-1.414-1.414L10 8.586 8.707 7.293z" clip-rule="evenodd" />
                                        </svg>
                                    </div>
                                    <div class="ml-3">
                                        <p class="text-sm text-red-800"><%= error %></p>
                                    </div>
                                </div>
                            </div>
                        <% } %>

                        <% if (success) { %>
                            <div class="mb-6 p-4 bg-green-50 border border-green-200 rounded-xl animate-bounce-in">
                                <div class="flex">
                                    <div class="flex-shrink-0">
                                        <svg class="h-5 w-5 text-green-400" viewBox="0 0 20 20" fill="currentColor">
                                            <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                                        </svg>
                                    </div>
                                    <div class="ml-3">
                                        <p class="text-sm text-green-800"><%= success %></p>
                                        <% if (apiResponse) { %>
                                            <div class="mt-3 p-3 bg-blue-50 rounded-lg">
                                                <h4 class="text-sm font-semibold text-blue-800 mb-2">ðŸŽ‰ Partner Profile Created Successfully!</h4>
                                                <div class="grid grid-cols-1 md:grid-cols-2 gap-2 text-xs text-blue-700">
                                                    <p><strong>Host ID:</strong> <%= apiResponse.data.hostProfile._id %></p>
                                                    <p><strong>User ID:</strong> <%= apiResponse.data.user._id %></p>
                                                    <p><strong>Profile Name:</strong> <%= apiResponse.data.hostProfile.ProfileName %></p>
                                                    <p><strong>Status:</strong> <%= apiResponse.data.user.isActive ? 'Active' : 'Inactive' %></p>
                                                    <p class="md:col-span-2"><strong>Created:</strong> <%= new Date(apiResponse.data.user.createdAt).toLocaleString() %></p>
                                                </div>
                                            </div>
                                        <% } %>
                                    </div>
                                </div>
                            </div>
                        <% } %>

                        <!-- Registration Form -->
